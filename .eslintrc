{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "no-console": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2

    # "arrow-body-style": 0,
    # "consistent-return": 0,
    # "eol-last": 0,
    # "global-require": 0,
    # "jsx-a11y/img-has-alt": 0,
    # "jsx-a11y/img-uses-alt": 0,
    # "max-len": 0,
    # "new-cap": 0,
    # "no-case-declarations": 0,
    # "no-confusing-arrow": 0,
    # "no-debugger": 0,
    # "no-param-reassign": 0,
    # "no-restricted-syntax": 0,
    # "no-underscore-dangle": 0,
    # "prefer-arrow-callback": 0,
    # "prefer-const": 0,
    # "prefer-template": 0,
    # "prop-types": 0,
    # "quote-props": 0,
    # "react/jsx-indent": 0,
    # "react/prop-types": 0,
    # "strict": 0,
    # "wrap-iife": 0,
  }
}
